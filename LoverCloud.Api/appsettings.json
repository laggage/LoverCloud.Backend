{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "MySql": "./connectionString.txt"
  },
  "JwtSettings": {
    "Audience": "LoverCloud",
    "Authority": "http://localhost:3088",
    "RequireHttpsMetadata": false,
    "TokenValidationParameters": {
      "ValidateAudience": true,
      "ValidAudience": "LoverCloud",
      "ValidateIssuer": true,
      "ValidIssuer": "http://localhost:3088",
      "ValidateLifetime": true,
      "ValidateIssuerSigningKey": true
    }
  },
  "CorSettings": {
    "PolicyName": "cors-ng",
    "Origins": [
      "http://localhost:4200",
      "http://*:4200"
    ],
    "ExposedHeaders": [ "X-Pagination" ]
  }
}
